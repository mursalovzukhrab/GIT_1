#include <iostream>

// Функция для вычисления n-го числа Фибоначчи (последовательность начинается с 0, 1)
int fib(int n) {
    if (n == 0)
        return 0;
    if (n == 1)
        return 1;
    int a = 0, b = 1, next;
    for (int i = 2; i <= n; ++i) {
        next = a + b;
        a = b;
        b = next;
    }
    return b;
}

int main() {
    // Вычисление и вывод 10-го числа Фибоначчи (начиная с 0, 1)
    int result = fib(10);
    std::cout << "10-ое число Фибоначчи (начиная с 0, 1): " << result << std::endl;

    return 0;
}
