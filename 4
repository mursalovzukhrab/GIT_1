#include <iostream>

// Функция для вычисления и вывода первых n чисел Фибоначчи (последовательность начинается с 0, 1)
void fib(int n) {
    int a = 0, b = 1, next;
    for (int i = 0; i < n; ++i) {
        std::cout << a << std::endl;
        next = a + b;
        a = b;
        b = next;
    }
}

int main() {
    // Вызов функции для вывода первых 10 чисел Фибоначчи (начиная с 0, 1)
    std::cout << "Первые 10 чисел Фибоначчи (начиная с 0, 1):" << std::endl;
    fib(10);

    return 0;
}
