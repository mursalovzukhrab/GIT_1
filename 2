#include <iostream>

// Функция для вычисления n-го числа Фибоначчи (последовательность начинается с 1, 1)
int fib(int n) {
    if (n <= 1)
        return n;
    return fib(n - 1) + fib(n - 2);
}

int main() {
    // Вычисление и вывод 10-го числа Фибоначчи (начиная с 1, 1)
    int result = fib(10);
    std::cout << "10-ое число Фибоначчи (начиная с 1, 1): " << result << std::endl;

    return 0;
}
